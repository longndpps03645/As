        if (e.keyCode == 13) {
            doSearch();
            return false;
        }
    });
});
function doSearch() {
   
    if ($("#txtQuery").val() == 'Nhập tên thương hiệu, mã hoặc đặc điểm sản phẩm')
        $("#txtQuery").val('');

    var searchUrl = "/tim-kiem.aspx?";
    var queryText = $("#txtQuery").val().toLowerCase();
    var queryTextNotVN = locdau(queryText);
    var queyTextSign = queryText;
    var categoryAutoID = $('#ddCategories').val();
    if (queryText.indexOf("'") > 0) {
        queyTextSign = queryTextNotVN;
    }
    var strReturn = "";
    var flag = 0;
    var ID = 0;
    if ($("#txtQuery").val() != '') {
        $.ajax({
            type: "POST",
            contentType: "application/json; charset=utf-8",
            url: "/Services/BiddingService.asmx/SearchReturnBrandName",
            dataType: "json",
            data: "{BrandName:'" + queryTextNotVN + "',BrandNameSign:'" + queyTextSign + "'}",
            async: false,
            success: function(msg) {
                if (msg.d == "0")
                    searchUrl = searchUrl + "brand=" + encodeURIComponent(queryText);

                if (msg.d == "1")
                    searchUrl = searchUrl + "q=" + encodeURIComponent(queryText);
            },
            error: function(xmlHttpRequest, status, err) {
            }

        });
    }
  
    window.location.href = searchUrl;
}


// default button

function clickButton2(e) {
    var evt = e ? e : window.event;
    if (evt.keyCode == 13) {
        doSearch()
        return false;
